export PATH=~/.cabal/bin:$PATH
export PATH=~/bin:$PATH:/usr/bin:/usr/sbin
export PATH=~/.rbenv/bin:$PATH
eval "$(rbenv init -)"
export PATH=~/.nodenv/bin:$PATH
eval "$(nodenv init -)"
export PYENV_ROOT=~/.pyenv
export PATH=$PYENV_ROOT/bin:$PATH
eval "$(pyenv init -)"
export PATH="$HOME/.luaenv/bin:$PATH"

export PATH="/home/cpup/.luaenv/shims:${PATH}"
export LUAENV_SHELL=zsh
source '/home/cpup/.luaenv/completions/luaenv.zsh'
luaenv rehash 2>/dev/null
luaenv() {
  local command
  command="$1"
  if [ "$#" -gt 0 ]; then
    shift
  fi

  case "$command" in
  rehash|shell)
    eval "`luaenv "sh-$command" "$@"`";;
  *)
    command luaenv "$command" "$@";;
  esac
}

export EDITOR=vim
which subl 2>&1 > /dev/null && export EDITOR=subl\ -w